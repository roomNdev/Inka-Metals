/*frame*/

@-webkit-keyframes slideInUp {
        0% {
                opacity: 0;
                -webkit-transform: translate3d(0, 70%, 0);
                transform: translate3d(0, 70%, 0);
        }
        100% {
                opacity: 1;
                -webkit-transform: none;
                transform: none;
        }
}

@keyframes fadeOut {
        0% {
                opacity: 1;
                -webkit-transform: translate3d(0, 0, 0);
                transform: translate3d(0, 0, 0);
        }
        100% {
                opacity: 0;
                -webkit-transform: none;
                transform: none;
        }
}
